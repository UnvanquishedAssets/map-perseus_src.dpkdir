// lights

// horizontal yellow
textures/perseus_eX/eX_light_u201_add_y
{
	qer_editorimage textures/perseus_eX/eX_light_u201_add.tga
	diffusemap textures/perseus_eX/eX_light_u201_d.tga
	normalmap addnormals(textures/perseus_eX/eX_light_u201_local.tga, heightmap(textures/perseus_eX/eX_light_u201_h.tga, 1))
	specularmap textures/perseus_eX/eX_light_u201_s.tga
	surfaceparm nomarks
	q3map_surfacelight 2000
	q3map_lightRGB 1 .8 .6
	{
		map textures/perseus_eX/eX_light_u201_add.tga
		blendfunc GL_ONE GL_ONE
	}
}

