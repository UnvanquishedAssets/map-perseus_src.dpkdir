// lights

// orange door
textures/perseus_pk01/pk01_door01a_d
{
	qer_editorimage textures/perseus_pk01/pk01_door01a_d.tga
	diffusemap textures/perseus_pk01/pk01_door01a_d.tga
	normalmap textures/perseus_pk01/pk01_door01_local.tga
	specularmap textures/perseus_pk01/pk01_door01_s.tga
